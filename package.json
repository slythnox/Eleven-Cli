{
  "name": "eleven-cli",
  "version": "1.0.0",
  "description": "Eleven - Your Personal AI Assistant CLI | Chat, Code, Create with Gemini AI",
  "main": "bin/eleven.js",
  "bin": {
    "eleven-cli": "./bin/eleven.js",
    "eleven": "./bin/eleven.js",
    "el": "./bin/eleven.js"
  },
  "scripts": {
    "start": "node bin/eleven.js",
    "install-global": "npm install -g .",
    "uninstall-global": "npm uninstall -g eleven-cli",
    "test": "node bin/eleven.js --help",
    "test:config": "node bin/eleven.js config --show",
    "test:health": "node bin/eleven.js health",
    "dev": "node bin/eleven.js",
    "link": "npm link",
    "unlink": "npm unlink",
    "setup": "chmod +x bin/eleven.js && npm link"
  },
  "dependencies": {
    "@google/generative-ai": "^0.15.0",
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "inquirer": "^8.2.6",
    "link": "^2.1.1",
    "ora": "^5.4.1"
  },
  "keywords": [
    "ai",
    "cli",
    "assistant",
    "gemini",
    "personal-assistant",
    "slythnox",
    "code-generation",
    "automation",
    "chat",
    "developer-tools",
    "productivity"
  ],
  "author": "Slythnox",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/slythnox/eleven-cli.git"
  },
  "files": [
    "bin/",
    "src/",
    "README.md"
  ]
}
